.data
finLigne:     .asciiz "\n"
              .align 2
.text
main :
           #Déclaration d'une nouvelle variable
   move $s7,$sp
   add $sp, $sp, -4
           #Affectation
   li $v0, 12
   sw $v0, 0($s7)
                   # affichage de l'expression
   li $v0, 36
    move $a0, $v0
    li $v0, 1
    syscall
    li $v0, 4      # retour à la ligne
    la $a0, finLigne
    syscall
end :
    li $v0, 10            # retour au système
    syscall

