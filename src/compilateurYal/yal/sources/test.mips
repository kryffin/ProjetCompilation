.data
finLigne:     .asciiz "\n"
              .align 2
.text
main :
	#initialiser $s7 avec $sp
	move $s7, $sp
	#reservation de la place memoire pour i
	add $sp, $sp, -4
	#reservation de la place memoire pour j
	add $sp, $sp, -4
	#affectation i = 30
    li $v0, 30
	move $a0, $v0 
	li $v0, $a0
	sw $v0, 0($s7)
	#affectation j = 50
    li $v0, 50
	move $a0, $v0 
	li $v0, $a0
	sw $v0, -4($s7)
                   # affichage de l'expression
    li $v0, 50
    move $a0, $v0
    li $v0, 1
    syscall
    li $v0, 4      # retour à la ligne
    la $a0, finLigne
    syscall
end :
    li $v0, 10            # retour au système
    syscall

